  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\StronglyApied\src\BreadTh.StronglyApied.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
